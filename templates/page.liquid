<div class="page-width">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
      </div>

      <div class="rte">
        {{ page.content }}
          <div style="width: 200px; height: 200px; background-color: #e95e61; color: black;">
            <ul>
                {% for product in collections['all'].products %}
                <li>{{ product.title }}</li>
                {% endfor %}
            </ul>
            <div>{{ shop.products_count }}</div>
            {% assign a = "Hello" %}
            {{ a }}
          </div>
      </div>
    </div>
  </div>
</div>