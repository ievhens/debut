                                            <div class="gwa-cpu-progress-bar-wrapper">
                                                <div class="gwa-cpu-progress-bar"></div>
                                            </div>

                                            <div class="gwa-cpu-status-text">
                                                <div class="gwa-cpu-text-status gwa-cpu-text-initial">
                                                    <p class="gwa-cpu-action-title">Drag & Drop</p>
                                                    <span class="gwa-cpu-action-subtitle">or</span>
                                                    <a class="gwa-cpu-action-link gwa-cpu-upload-manually"><span class="gwa-cpu-upload-manually-desktop">upload manually</span><span class="gwa-cpu-upload-manually-mob">choose your photo</span></a>
                                                </div>
                                                <div class="gwa-cpu-text-status gwa-cpu-text-drag">
                                                    <p class="gwa-cpu-action-title">drop your .jpg, .png or .gif files here</p>
                                                </div>
                                                <div class="gwa-cpu-text-status gwa-cpu-text-form">
                                                    <p class="gwa-cpu-action-title">Fill in the form</p>
                                                    <p class="gwa-cpu-action-subtitle">to upload selected file</p>
                                                    <a class="gwa-cpu-action-link gwa-cpu-upload-manually">change your photo</a>
                                                </div>
                                                <div class="gwa-cpu-text-status gwa-cpu-text-confirm">
                                                    <p class="gwa-cpu-action-title">Confirm</p>
                                                    <p class="gwa-cpu-action-subtitle">or</p>
                                                    <a class="gwa-cpu-action-link gwa-cpu-upload-manually">change your photo</a>
                                                </div>
                                                <div class="gwa-cpu-text-status gwa-cpu-text-uploading">
                                                    <p class="gwa-cpu-action-title">Photo uploading</p>
                                                </div>
                                                <div class="gwa-cpu-text-status gwa-cpu-text-uploaded">
                                                    <p class="gwa-cpu-action-title">Complete!</p>
                                                    <a class="gwa-cpu-action-link gwa-cpu-upload-more">Upload more</a>
                                                </div>
                                            </div>
                                        </div>
                                </div>
                            </div>
                            <div class="gwa-cpu-upload-info">
                                <div class="gwa-cpu-upload-info__types">Allowed file types: jpg, jpeg, gif, png.</div>
                                <div class="gwa-cpu-upload-info__size">Maximum file size: 10Mb.</div>
                            </div>
                            <div class="gwa-cpu-thankUMessage">{{ cpu_upload_settings.thankYouMessage.text }}</div>
                            <p class="gwa-cpu-error"></p>
                            {% capture gwa_cpu_input_error %}
                                <p class="gwa-cpu-input-error">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20">
                                        <path fill="#bf0711" fill-rule="evenodd" d="M10 18a8 8 0 1 1 0-16 8 8 0 0 1 0 16zm-1-8h2V6H9v4zm0 4h2v-2H9v2z"/>
                                    </svg>
                                    <span class="gwa-cpu-input-error-text"></span>
                                </p>
                            {% endcapture %}
                            <div class="gwa-cpu-form-wrapper">
                                <form class="gwa-cpu-form" novalidate>
                                    <input class="gwa-cpu-upload-file" type="file" name="image" accept="image/gif, image/jpeg, image/png" style="display: none;"/>
                                    <input type="hidden" name="pageData[cartToken]" value="{{ cpu_cart_token }}"/>
                                    <input type="hidden" name="pageData[entityType]" value="{{ cpu_widget_type }}"/>
                                    {% if cpu_product %}
                                    <input type="hidden" name="pageData[entityId]" value="{{ cpu_product.id  }}"/>
                                    {% endif %}
                                    {% if customer %}
                                        <input type="hidden" name="pageData[customer][id]" value="{{ customer.id }}"/>
                                        <input type="hidden" name="pageData[customer][email]" value="{{ customer.email }}"/>
                                        <input type="hidden" name="pageData[customer][has_account]" value="{{ customer.has_account }}"/>
                                    {% endif %}
                                    {% if cpu_form_settings.customerEmail.enabled %}
                                        <div class="gwa-cpu-form-field {% if customer %}gwa-cpu-field-valid{% endif %}">
                                            <label class="gwa-cpu-form-label">
                                                <span class="gwa-cpu-form-label-text">{{ cpu_form_settings.customerEmail.field.title }}</span>
                                                <input name="formData[customerEmail]" placeholder="{{ cpu_form_settings.customerEmail.field.settings.placeholder }}" class="gwa-cpu-input gwa-cpu-input-text" type="email"
                                                       {% if cpu_form_settings.customerEmail.field.settings.required %}required{% endif %}
                                                       {% if cpu_form_settings.sendNotification.enabled and cpu_form_settings.sendNotification.field.settings.defaultValue %}data-required="true"{% endif %}
                                                       {% if customer %}value="{{ customer.email }}"{% endif %}/>
                                            </label>
                                            {{ gwa_cpu_input_error }}
                                        </div>
                                    {% endif %}
                                    {% if cpu_form_settings.alt.enabled %}
                                        <div class="gwa-cpu-form-field">
                                            <label class="gwa-cpu-form-label">
                                                <span class="gwa-cpu-form-label-text">{{ cpu_form_settings.alt.field.title }}</span>
                                                <input name="formData[alt]" placeholder="{{ cpu_form_settings.alt.field.settings.placeholder }}" {% if cpu_form_settings.alt.field.settings.required %}required{% endif %} class="gwa-cpu-input gwa-cpu-input-text" type="text"/>
                                            </label>
                                            {{ gwa_cpu_input_error }}
                                        </div>
                                    {% endif %}
                                    {% assign first_checkbox = false %}
                                    {% if cpu_form_settings.sendNotification.enabled and cpu_form_settings.customerEmail.enabled %}
                                        {% assign first_checkbox = true %}
                                        <div class="gwa-cpu-form-field gwa-cpu-form-checkbox gwa-cpu-form-checkbox-first">
                                            <label class="gwa-cpu-form-label">
                                                <input name="formData[sendNotification]" class="gwa-cpu-input gwa-cpu-input-checkbox" type="checkbox" value="on" {% if cpu_form_settings.sendNotification.field.settings.defaultValue %}checked{% endif %}/>
                                                <span class="gwa-cpu-checkbox-custom">
                                                    <svg class="gwa-cpu-checkmark" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" viewBox="0 0 16 16">
                                                        <path fill="#5C6AC4" d="M6.315 11.858L3.133 8.441a.506.506 0 0 1 0-.684l.643-.683a.437.437 0 0 1 .642 0l2.219 2.393L11.58 4.14a.437.437 0 0 1 .643 0l.643.684a.504.504 0 0 1 0 .683l-5.91 6.35a.437.437 0 0 1-.642 0"/>
                                                    </svg>
                                                </span>
                                                <span class="gwa-cpu-form-label-text">{{ cpu_form_settings.sendNotification.field.title }}</span>
                                            </label>
                                            {{ gwa_cpu_input_error }}
                                        </div>
                                    {% endif %}
                                    {% if cpu_form_settings.agreement.enabled %}
                                        <div class="gwa-cpu-form-field gwa-cpu-form-checkbox {% unless first_checkbox %}gwa-cpu-form-checkbox-first{% endunless %}">
                                            <label class="gwa-cpu-form-label">
                                                <input name="formData[agreement]" class="gwa-cpu-input gwa-cpu-input-checkbox" type="checkbox" value="on" {% if cpu_form_settings.agreement.field.settings.defaultValue %}checked{% endif %} {% if cpu_form_settings.agreement.field.settings.required %}required{% endif %}>
                                                <span class="gwa-cpu-checkbox-custom">
                                                    <svg class="gwa-cpu-checkmark" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" viewBox="0 0 16 16">
                                                        <path fill="#5C6AC4" d="M6.315 11.858L3.133 8.441a.506.506 0 0 1 0-.684l.643-.683a.437.437 0 0 1 .642 0l2.219 2.393L11.58 4.14a.437.437 0 0 1 .643 0l.643.684a.504.504 0 0 1 0 .683l-5.91 6.35a.437.437 0 0 1-.642 0"/>
                                                    </svg>
                                                </span>
                                                <span class="gwa-cpu-form-label-text">{{ cpu_form_settings.agreement.field.title }}</span>
                                            </label>
                                            {{ gwa_cpu_input_error }}
                                        </div>
                                    {% endif %}
                                    <div class="gwa-cpu-submit-container">
                                        <button type="submit" class="gwa-cpu-upload-submit gwa-cpu-disabled">{{ cpu_upload_settings.buttonText }}</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
        </div>
        {% endif %}
    </div>
{% endif %}