{% assign key_upload_name = 'upload_' | append: theme.id %}
{% assign cpu_upload_settings = shop.metafields.gwa[key_upload_name] %}
{% assign key_form_name = 'form_' | append: theme.id %}
{% assign cpu_form_settings = shop.metafields.gwa[key_form_name] %}

{% if cpu_product %}
    {% assign key_name = 'product_' | append: theme.id %}
    {% assign cpu_images = cpu_product.metafields['gwa']['customer_photos'] | split: '||' %}
    {% assign cpu_widget_type = 'product' %}
{% else %}
    {% assign key_name = 'home_' | append: theme.id %}
    {% assign cpu_images = shop.metafields['gwa']['customer_photos'] | split: '||' %}
    {% assign cpu_widget_type = 'index' %}
{% endif %}
{% assign cpu_settings = shop.metafields.gwa[key_name] %}
{% assign cpu_cart_json = cart | json %}
{% assign cpu_cart_token = cpu_cart_json | split:'token":"' | last | split:'"' | first %}
{%- capture cpu_data -%}{"shop":"{{ shop.permanent_domain }}", "entityType":"{{ cpu_widget_type }}", {% if cpu_product %}"entityId":"{{ cpu_product.id }}",{% endif %} "cartToken":"{{ cpu_cart_token }}", "imagesCount":{{ cpu_images.size }}, "cid":{% if customer %}"{{ customer.id }}"{% else %}null{% endif %} }{% endcapture %}
{% assign form_enabled = false %}
{% if cpu_form_settings.customerEmail.enabled or cpu_form_settings.alt.enabled or cpu_form_settings.agreement.enabled %}
    {% assign form_enabled = true %}
{% endif %}

{% if cpu_settings %}

    {% if cpu_images.size > 0 %}
        {% if cpu_settings.header.text_with_photos != blank %}
            {% assign header_text = cpu_settings.header.text_with_photos %}
        {% endif %}
        {% if cpu_settings.sub_header.text_with_photos %}
            {% assign subheader_text = cpu_settings.sub_header.text_with_photos %}
        {% endif %}
        {% if cpu_settings.paragraph.text_with_photos != blank %}
            {% assign paragraph_text = cpu_settings.paragraph_text_with_photos %}
        {% endif %}
    {% else %}
        {% if cpu_settings.header.text_without_photos != blank %}
            {% assign header_text = cpu_settings.header.text_without_photos %}
        {% endif %}
        {% if cpu_settings.sub_header.text_without_photos != blank %}
            {% assign subheader_text = cpu_settings.sub_header.text_without_photos %}
        {% endif %}
        {% if cpu_settings.paragraph.text_without_photos != blank %}
            {% assign paragraph_text = cpu_settings.paragraph.text_without_photos %}
        {% endif %}
    {% endif %}

    <style>
        .gwa-cpu-widget {
            padding: 20px;
            margin: 20px auto;
            transition: opacity 1.5s linear 1s, visibility 1.5s linear 1s;
            opacity: 1;
            visibility: visible;
            max-height: 100%;
            width: 100%;
        }
        .gwa-cpu-widget.gwa-cpu-loading {
            opacity: 0;
            visibility: hidden;
            max-height: 0px;
            overflow: hidden;
        }
        {% unless cpu_settings.global.full_width %}
        .gwa-cpu-widget-{{ cpu_widget_type }} {
            max-width: {{ cpu_settings.global.custom_width }}px;
        }
        {% endunless %}
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-title {
            text-align: {{ cpu_settings.header.horizontal_alignment.desktop }};
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-subheader {
            text-align: {{ cpu_settings.sub_header.horizontal_alignment.desktop }};
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-paragraph {
            text-align: {{ cpu_settings.paragraph.horizontal_alignment.desktop }};
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button-block {
            text-align: {{cpu_settings.upload_button.horizontal_alignment.desktop }};
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-line {
            border-color: rgba({{ cpu_settings.line.color.rgba }});
        }
        {% unless cpu_settings.global.use_theme_styles %}
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-title {
            margin: 0;
            font-size: {{ cpu_settings.header.font_size }}px;
            color: rgba({{ cpu_settings.header.color.rgba }});
            padding-top: {{ cpu_settings.header.padding_top }}px;
            padding-bottom: {{ cpu_settings.header.padding_bottom }}px;
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-subheader {
            margin: 0;
            font-size: {{ cpu_settings.sub_header.font_size }}px;
            color: rgba({{ cpu_settings.sub_header.color.rgba }});
            padding-top: {{ cpu_settings.sub_header.padding_top }}px;
            padding-bottom: {{ cpu_settings.sub_header.padding_bottom }}px;
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-paragraph {
            margin: 0;
            font-size: {{ cpu_settings.paragraph.font_size }}px;
            color: rgba({{ cpu_settings.paragraph.color.rgba }});
            padding-top: {{ cpu_settings.paragraph.padding_top }}px;
            padding-bottom: {{ cpu_settings.paragraph.padding_bottom }}px;
        }
        {% endunless %}
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-paragraph {
            width: {{ cpu_settings.paragraph.width.desktop }}%;
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-subheader-block {
            width: {{ cpu_settings.sub_header.width.desktop }}%;
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button-block {
            padding-top: {{ cpu_settings.upload_button.padding_top }}px;
            padding-bottom: {{ cpu_settings.upload_button.padding_bottom }}px;
            width: {{ cpu_settings.upload_button.width.desktop }}%;
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button {
            font-size: {{ cpu_settings.upload_button.font_size }}px;
            color: rgba({{ cpu_settings.upload_button.text_color.rgba }});
            background-color: rgba({{ cpu_settings.upload_button.background_color.rgba }});
            border-color: rgba({{ cpu_settings.upload_button.border_color.rgba }});
            border-radius: {{ cpu_settings.upload_button.border.radius }}px;
            border-width: {{ cpu_settings.upload_button.border.width }}px;
            padding: {{ cpu_settings.upload_button.padding_inner_top }}px {{ cpu_settings.upload_button.padding_inner_left }}px;
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button:hover {
            color: rgba({{ cpu_settings.upload_button.hover_text_color.rgba }});
            background-color: rgba({{ cpu_settings.upload_button.hover_background_color.rgba }});
            border-color: rgba({{ cpu_settings.upload_button.hover_border_color.rgba }});
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-flex {
            align-items: {{ cpu_settings.upload_button.vertical_alignment }};
        }
        .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-dots {
            text-align: {{ cpu_settings.slider.dots_alignment }} ;
        }
        @media screen and (max-width: 960px) {
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-title {
                text-align: {{ cpu_settings.header.horizontal_alignment.tablet }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-subheader {
                text-align: {{ cpu_settings.sub_header.horizontal_alignment.tablet }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-paragraph {
                text-align: {{ cpu_settings.paragraph.horizontal_alignment.tablet }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button-block {
                text-align: {{cpu_settings.upload_button.horizontal_alignment.tablet }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-subheader-block {
                width: {{ cpu_settings.sub_header.width.tablet }}%;
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-paragraph {
                width: {{ cpu_settings.paragraph.width.tablet }}%;
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button-block {
                width: {{ cpu_settings.upload_button.width.tablet }}%;
            }
        }
        @media screen and (max-width: 480px) {
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-title {
                text-align: {{ cpu_settings.header.horizontal_alignment.mobile }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-subheader {
                text-align: {{ cpu_settings.sub_header.horizontal_alignment.mobile }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-paragraph {
                text-align: {{ cpu_settings.paragraph.horizontal_alignment.mobile }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button-block {
                text-align: {{cpu_settings.upload_button.horizontal_alignment.mobile }};
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-subheader-block {
                width: {{ cpu_settings.sub_header.width.mobile }}%;
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-paragraph {
                width: {{ cpu_settings.paragraph.width.mobile }}%;
            }
            .gwa-cpu-widget-{{ cpu_widget_type }} .gwa-cpu-widget-button-block {
                width: {{ cpu_settings.upload_button.width.mobile }}%;
            }
        }
    </style>
    <div class="gwa-clearfix"></div>
    <div class="gwa-cpu-widget gwa-cpu-widget-{{ cpu_widget_type }} gwa-cpu-loading" itemscope itemtype="http://schema.org/ImageGallery" data-cpu-data='{{ cpu_data }}' {% if section.id %}data-cpu-section-id="{{ section.id }}"{% endif %} data-cpu-styles="{{ 'gwa-cpu.scss.css' | asset_url }}">

        {% if cpu_settings.header.show and header_text and cpu_settings.header.position == 'above_slider' %}
            <{{ cpu_settings.header.tag_type }} class="gwa-cpu-widget-title">
                {{ header_text }}
            </{{ cpu_settings.header.tag_type }}>
        {% endif %}

        <div class="gwa-cpu-flex">
            {% if cpu_settings.sub_header.show and subheader_text and cpu_settings.sub_header.position == 'above_slider' %}
                <div class="gwa-cpu-widget-subheader-block">
                    <{{ cpu_settings.sub_header.tag_type }} class="gwa-cpu-widget-subheader">{{ subheader_text }}</{{ cpu_settings.sub_header.tag_type }}>
                </div>
            {% endif %}

            {% if cpu_settings.upload_button.show and cpu_settings.upload_button.position == 'above_slider' %}
                <div class="gwa-cpu-widget-button-block">
                    <button class="gwa-cpu-widget-button">{{ cpu_settings.upload_button.text }}</button>
                </div>
            {% endif %}
        </div>

        {% if cpu_settings.paragraph.show and paragraph_text and cpu_settings.paragraph.position == 'above_slider' %}
            <div>
                <{{ cpu_settings.paragraph.tag_type }} class="gwa-cpu-widget-paragraph">{{ paragraph_text }}</{{ cpu_settings.paragraph.tag_type }}>
            </div>
        {% endif %}

        {% if cpu_images.size > 0 %}
            <div class="gwa-cpu-slider" itemprop="name" content="{{ header_text }}" data-cpu-slider='{{ cpu_settings.slider | json }}'>
                {% for cpu_image in cpu_images %}
                    {% assign cpu_image_data = cpu_image | split: '|' %}
                    <div class="gwa-cpu-slide-wrapper" style="display: inline-block;" itemscope itemtype="http://schema.org/ImageObject"><img class="gwa-cpu-slide" title="{{ cpu_image_data[0] }}" alt="{{ cpu_image_data[1] }}" style="" src="{{ cpu_image_data[2] }}" data-full-image-src="{{ cpu_image_data[3] }}" data-full-image-width="{{ cpu_image_data[4] }}" data-full-image-height="{{ cpu_image_data[5] }}" data-slide-index="{{ forloop.index0 }}" data-image-id="{{ cpu_image_data[6] }}" itemprop="thumbnailUrl"></div>
                {% endfor %}
            </div>
        {% endif %}

        {% if cpu_settings.header.show and header_text and cpu_settings.header.position == 'below_slider' %}
            <{{ cpu_settings.header.tag_type }} class="gwa-cpu-widget-title">{{ header_text }}</{{ cpu_settings.header.tag_type }}>
        {% endif %}

        <div class="gwa-cpu-flex">
            {% if cpu_settings.sub_header.show and subheader_text and cpu_settings.sub_header.position == 'below_slider' %}
                <div class="gwa-cpu-widget-subheader-block">
                    <{{ cpu_settings.sub_header.tag_type }} class="gwa-cpu-widget-subheader">{{ subheader_text }}</{{ cpu_settings.sub_header.tag_type }}>
                </div>
            {% endif %}
            {% if cpu_settings.upload_button.show and cpu_settings.upload_button.position == 'below_slider' %}
                <div class="gwa-cpu-widget-button-block">
                    <button class="gwa-cpu-widget-button">{{ cpu_settings.upload_button.text }}</button>
                </div>
            {% endif %}
        </div>

        {% if cpu_settings.paragraph.show and paragraph_text and cpu_settings.paragraph.position == 'below_slider' %}
            <div>
                <{{ cpu_settings.paragraph.tag_type }} class="gwa-cpu-widget-paragraph">{{ paragraph_text }}</{{ cpu_settings.paragraph.tag_type }}>
            </div>
        {% endif %}

        <div class="gwa-cpu-widget-footer">
            {% if cpu_settings.line.show %}
                <div class="gwa-cpu-line"></div>
            {% endif %}
            <div class="gwa-cpu-dots"></div>
        </div>

        {% if  cpu_settings.lightbox.show %}
        <style>
            .gwa-cpu-lightbox-{{ cpu_widget_type }} .cpu-pswp__bg {
                background-color: rgba({{ cpu_settings.lightbox.background_color.rgba }});
            }
            .cpu-pswp__ui--hidden .cpu-pswp__top-bar, .cpu-pswp__ui--hidden .cpu-pswp__caption, .cpu-pswp__ui--hidden .cpu-pswp__button--arrow--left, .cpu-pswp__ui--hidden .cpu-pswp__button--arrow--right {
                opacity: 0.001;
            }
        </style>
        <div class="cpu-pswp gwa-cpu-lightbox gwa-cpu-lightbox-{{ cpu_widget_type }}" tabindex="-1" role="dialog" aria-hidden="true"
             data-settings='{
                 "share": "{{ cpu_settings.lightbox.share }}",
                 "caption": "{{ cpu_settings.lightbox.caption }}",
                 "bgOpacity": "{{ cpu_settings.lightbox.background_color.opacity | times: 100 }}"
                 }'>
            <div class="cpu-pswp__bg"></div>
            <div class="cpu-pswp__scroll-wrap">
                <div class="cpu-pswp__container">
                    <div class="cpu-pswp__item"></div>
                    <div class="cpu-pswp__item"></div>
                    <div class="cpu-pswp__item"></div>
                </div>
                <div class="cpu-pswp__ui cpu-pswp__ui--hidden">
                    <div class="cpu-pswp__top-bar">
                        <div class="cpu-pswp__counter"></div>
                        <button class="cpu-pswp__button cpu-pswp__button--close" title="Close (Esc)"></button>
                        <button class="cpu-pswp__button cpu-pswp__button--share" title="Share"></button>
                        <button class="cpu-pswp__button cpu-pswp__button--fs" title="Toggle fullscreen"></button>
                        <button class="cpu-pswp__button cpu-pswp__button--zoom" title="Zoom in/out"></button>
                        <div class="cpu-pswp__preloader">
                            <div class="cpu-pswp__preloader__icn">
                                <div class="cpu-pswp__preloader__cut">
                                    <div class="cpu-pswp__preloader__donut"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="cpu-pswp__share-modal cpu-pswp__share-modal--hidden cpu-pswp__single-tap">
                        <div class="cpu-pswp__share-tooltip"></div>
                    </div>
                    <button class="cpu-pswp__button cpu-pswp__button--arrow--left" title="Previous (arrow left)">
                    </button>
                    <button class="cpu-pswp__button cpu-pswp__button--arrow--right" title="Next (arrow right)">
                    </button>
                    <div class="cpu-pswp__caption">
                        <div class="cpu-pswp__caption__center"></div>
                    </div>
                </div>
            </div>
        </div>
        {% endif %}

        {% if cpu_settings.upload_button.show %}
        <style>
            .gwa-cpu-thankUMessage {
                font-size: {{ cpu_upload_settings.thankYouMessage.fontSize }}px;
            }
            .gwa-cpu-upload-submit {
                font-size: {{ cpu_upload_settings.buttonFontSize }}px;
                line-height: 1;
                color: rgba({{ cpu_upload_settings.buttonTextColor.rgba }});
                background-color: rgba({{ cpu_upload_settings.buttonBackgroundColor.rgba }});
                border-color: rgba({{ cpu_upload_settings.buttonBorder.color.rgba }});
                border-radius: {{ cpu_upload_settings.buttonBorder.radius }}px;
                border-width: {{ cpu_upload_settings.buttonBorder.width }}px;
                padding: {{ cpu_upload_settings.buttonPaddingInnerTopBottom }}px {{ cpu_upload_settings.buttonPaddingInnerLeftRight }}px;
                margin-top: {{ cpu_upload_settings.buttonPaddingTop }}px;
                margin-bottom: {{ cpu_upload_settings.buttonPaddingBottom }}px;
                width: 100%;
                max-width: {{ cpu_upload_settings.buttonSize.width }}px;
                transition: all 0.3s;
            }
            .gwa-cpu-upload-submit:hover {
                color: rgba({{ cpu_upload_settings.buttonTextColorHover.rgba }});
                background-color: rgba({{ cpu_upload_settings.buttonBackgroundColorHover.rgba }});
                border-color: rgba({{ cpu_upload_settings.buttonBorderColorHover.rgba }});
            }
            {% if cpu_upload_settings.showPopupBackground %}
                .gwa-cpu-modal {
                    background-color: rgba({{ cpu_upload_settings.popupBackgroundColor.rgba }});
                }
            {% endif %}
        </style>
        <div class="gwa-cpu-modal gwa-cpu-widget-upload" style="display:none;" data-stage="load_image" data-state="none" data-file-type="none" data-widget-form="{{ form_enabled }}">
                <div class="gwa-cpu-modal-container">
                    <div class="gwa-cpu-modal-inner">
                        <div class="gwa-cpu-modal-close-wrapper">
                            <span class="gwa-cpu-upload-cancel gwa-cpu-modal-close">
                                <svg xmlns="http://www.w3.org/2000/svg" width="17" height="17" viewBox="0 0 17 17">
                                    <g fill="none" fill-rule="evenodd" stroke="#DEE1E6" stroke-width="2">
                                        <path d="M1 1l15 15M1 16L16 1"/>
                                    </g>
                                </svg>
                            </span>
                        </div>
                        <div class="gwa-cpu-modal-content">
